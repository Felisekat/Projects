import turtle

turnt = turtle.Turtle()
turnt.shape("turtle")
#turns = []


turns = [int(input("Enter a whole number: "))for _ in range(5)]

for direction in turns: #declaring variable directions to read each input in turns list 
    turnt.left(direction)
    turnt.forward(100)


